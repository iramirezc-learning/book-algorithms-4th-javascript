    "jasmine": "^3.10.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jsdoc": "~3.6.6",
    "jsdom": "^16.4.0",
    "mathjs": "^8.0.1",
    "prettier": "^2.2.1"
  }
